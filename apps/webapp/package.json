{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --clearScreen false",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "router": "tsr watch",
    "codegen": "NODE_TLS_REJECT_UNAUTHORIZED=0 graphql-codegen",
    "codegen:watch": "NODE_TLS_REJECT_UNAUTHORIZED=0 graphql-codegen --watch",
    "cert": "rm -rf .cert && mkdir -p .cert && mkcert -key-file ./.cert/key.pem -cert-file ./.cert/cert.pem 'localhost'"
  },
  "dependencies": {
    "@apollo/client": "^3.10.1",
    "@graphql-codegen/cli": "^5.0.2",
    "@guesthub/ui": "workspace:*",
    "@hookform/resolvers": "^3.3.4",
    "@parcel/watcher": "^2.4.1",
    "@tanstack/react-router": "^1.31.1",
    "@tanstack/react-table": "^8.16.0",
    "@tanstack/router-cli": "^1.30.0",
    "@tanstack/router-devtools": "^1.31.1",
    "@tanstack/router-vite-plugin": "^1.30.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "graphql": "^16.8.1",
    "graphql-config": "^5.0.3",
    "jotai": "^2.8.0",
    "lucide-react": "^0.376.0",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "7.51.3",
    "sonner": "^1.4.41",
    "tailwind-merge": "^2.3.0",
    "usehooks-ts": "^3.1.0",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.12.1",
    "@graphql-codegen/client-preset": "^4.2.5",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@guesthub/tsconfig": "workspace:*",
    "@guesthub/eslint-config": "workspace:*",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-mkcert": "^1.17.5"
  }
